{
  "name": "Mon container Codespace",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "containerEnv": {
    "YOUR_SECRET_EMAIL": "${localEnv:YOUR_SECRET_EMAIL}",
    "YOUR_SECRET_PASSWORD": "${localEnv:YOUR_SECRET_PASSWORD}",
    "YOUR_SECRET_LOGON_URL": "${localEnv:YOUR_SECRET_LOGON_URL}",
    "YOUR_SECRET_PLANNING_URL": "${localEnv:YOUR_SECRET_PLANNING_URL}",
    "YOUR_SECRET_LOGIN_URL": "${localEnv:YOUR_SECRET_LOGIN_URL}",
    "YOUR_SECRET_MY_NAME": "${localEnv:YOUR_SECRET_MY_NAME}"
  },
  "postCreateCommand": "bash setup.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "GitHub.copilot"
      ],
      "settings": {
       "python.defaultInterpreterPath": "/usr/local/bin/python",
      "locale": "en"
   }
  }
},
  "remoteUser": "vscode"
}